- if @query_data.empty? then
  %p No data found
- else
  .table-responsive
    %table.table.table-hover
      %thead
        %tr
          - @query_data.first.each_key do |key|
            %th #{ key }
      %tbody
        - @query_data.each do |line|
          %tr
            - line.each_value do |datum|
              %td #{ datum }
